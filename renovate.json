{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^Dockerfile$"
      ],
      "matchStrings": [
        "ARG GENMON_VERSION=(?<currentValue>.*?)\n"
      ],
      "depNameTemplate": "jgyates/genmon",
      "datasourceTemplate": "github-releases"
    }
  ]
}
