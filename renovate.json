{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ]
  "regexManagers": [
    {
      "fileMatch": ["/Dockerfile$"],
      "matchStrings": [
        "ARG GENMON_VERSION=\"(?<currentValue>.+?)\"\\s+"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "jgyates/genmon"
    }
  ],
}
